  "item": [
    {
      "name": "üîê User Login",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": {
          "raw": "{{base_url}}/api/auth/login/",
          "host": [
            "{{base_url}}"
          ],
          "path": [
            "api",
            "auth",
            "login"
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\n  \"email\": \"{{test_user_email}}\",\n  \"password\": \"{{test_user_password}}\"\n}",
          "options": {
            "raw": {
              "language": "json"
            }
          }
        }
      },
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test('Login successful', function () { pm.response.to.have.status(200); });",
              "pm.test('Response has access token', function () { pm.expect(pm.response.json()).to.have.property('access'); });",
              "pm.test('Response has refresh token', function () { pm.expect(pm.response.json()).to.have.property('refresh'); });"
            ],
            "type": "text/javascript"
          }
        }
      ]
    },
